<!-- UserView.fxml -->
<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml" fx:controller="photo68.view.UserController">
    <ListView fx:id="albumListView"/>
    <Button text="Create Album" onAction="#createAlbum"/>
    <Button text="Delete Album" onAction="#deleteAlbum"/>
    <Button text="Open Album" onAction="#openAlbum"/>
</VBox>
