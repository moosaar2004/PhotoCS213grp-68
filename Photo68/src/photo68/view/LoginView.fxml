<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.text.*?>
<?import javafx.geometry.*?>

<GridPane xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml" fx:controller="photo68.view.LoginController" prefWidth="400" prefHeight="250" alignment="CENTER">
    
    <Label text="Username:" GridPane.columnIndex="0" GridPane.rowIndex="1"/>
    <TextField fx:id="usernameField" promptText="Enter username" GridPane.columnIndex="1" GridPane.rowIndex="1" GridPane.columnSpan="2"/>
    
    <Button fx:id="login" text="Login" onAction="#login" GridPane.columnIndex="1" GridPane.rowIndex="2" GridPane.columnSpan="2"/>
    
   <padding>
		<Insets top="10" right="10" bottom="10" left="10"/>
	</padding>
</GridPane>