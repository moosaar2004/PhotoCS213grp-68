<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="rootPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml" fx:controller="photo68.view.UserController">

    <ListView fx:id="albumListView" AnchorPane.topAnchor="10.0" AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="10.0" AnchorPane.bottomAnchor="50.0" />

    <Button fx:id="createButton" text="Create Album" onAction="#createAlbum" AnchorPane.leftAnchor="10.0" AnchorPane.bottomAnchor="10.0" />

    <Button fx:id="deleteButton" text="Delete Album" onAction="#deleteAlbum" AnchorPane.leftAnchor="120.0" AnchorPane.bottomAnchor="10.0" />

    <Button fx:id="openButton" text="Open Album" onAction="#openAlbum" AnchorPane.leftAnchor="230.0" AnchorPane.bottomAnchor="10.0" />

    <Button fx:id="renameButton" text="Rename Album" onAction="#renameAlbum" AnchorPane.leftAnchor="340.0" AnchorPane.bottomAnchor="10.0" />

    <Button fx:id="logout" text="Logout" onAction="#logout" AnchorPane.rightAnchor="10.0" AnchorPane.bottomAnchor="10.0" />

</AnchorPane>
